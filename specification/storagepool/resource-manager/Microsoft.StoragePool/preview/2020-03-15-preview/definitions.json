          }
        },
        "nextLink": {
          "description": "URI to fetch the next section of the paginated response.",
          "type": "string",
          "readOnly": true
        }
      }
    },
    "DiskPool": {
      "type": "object",
      "description": "Request payload for Create or Update Disk Pool requests.",
      "properties": {
        "properties": {
          "x-ms-client-flatten": true,
          "description": "RP defined properties for Disk Pool operations.",
          "$ref": "#/definitions/DiskPoolProperties"
        },
        "sku": {
          "description": "Sku description.",
          "$ref": "./common-types.json#/definitions/Sku"
        }
      },
      "allOf": [
        {
          "$ref": "./common-types.json#/definitions/TrackedResource"
        }
      ]
    },
    "AvailabilityZone": {
      "type": "string",
      "description": "Availability zone"
    },
    "DiskPoolProperties": {
      "type": "object",
      "description": "Properties section of the Disk Pool request payload.",
      "properties": {
        "provisioningState": {
          "type": "string",
          "readOnly": true,
          "enum": [
            "Invalid",
            "Succeeded",
            "Failed",
            "Canceled",
            "Pending"
          ],
          "x-ms-enum": {
            "name": "ProvisioningStates",
            "modelAsString": true
          },
          "description": "State of provisioning of the Disk pool"
        },
        "availabilityZones": {
          "type": "array",
          "description": "Logical zone for DiskPool resource.",
          "items": {
            "$ref": "#/definitions/AvailabilityZone"
          }
        },
        "status": {
          "type": "string",
          "readOnly": true,
          "enum": [
            "Invalid",
            "Unknown",
            "Healthy",
            "Unhealthy"
          ],
          "x-ms-enum": {
            "name": "OperationalStatus",
            "modelAsString": true
          },
          "description": "Operational status of the Disk pool"
        },
        "disks": {
          "type": "array",
          "description": "List of Azure managed disks to attach to a DiskPool",
          "items": {
            "$ref": "#/definitions/Disk"
          },
          "minLength": 1,
          "maxLength": 15
        }
      }
    },
    "Disk": {
      "type": "object",
      "description": "Managed disk to attach to the DiskPool. Required.",
      "required": [
        "id"
      ],
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique Azure resource id of the managed disk. Required."
        }
      }
    },
    "IscsiTargetList": {
      "type": "object",
      "required": [
        "value"
      ],
      "description": "List of iSCSI Targets.",
      "additionalProperties": false,
      "properties": {
