          "type": "array",
          "description": "An array of iSCSI targets within a Disk Pool.",
          "items": {
            "$ref": "#/definitions/IscsiTarget"
          }
        },
        "nextLink": {
          "description": "URI to fetch the next section of the paginated response.",
          "type": "string",
          "readOnly": true
        }
      }
    },
    "IscsiTarget": {
      "type": "object",
      "description": "Payload for iSCSI Target Create or Update requests.",
      "properties": {
        "properties": {
          "x-ms-client-flatten": true,
          "description": "RP defined properties for iSCSI Target operations.",
          "$ref": "#/definitions/IscsiTargetProperties"
        }
      },
      "allOf": [
        {
          "$ref": "./common-types.json#/definitions/ProxyResource"
        }
      ]
    },
    "IscsiTargetProperties": {
      "type": "object",
      "description": "Properties of iSCSI Target request payload.",
      "properties": {
        "provisioningState": {
          "type": "string",
          "enum": [
            "Invalid",
            "Succeeded",
            "Failed",
            "Canceled",
            "Pending"
          ],
          "x-ms-enum": {
            "name": "ProvisioningStates",
            "modelAsString": true
          },
          "description": "State of provisioning of the iSCSI target",
          "x-ms-mutability": [
            "read"
          ]
        },
        "status": {
          "type": "string",
          "enum": [
            "Invalid",
            "Unknown",
            "Healthy",
            "Unhealthy"
          ],
          "x-ms-enum": {
            "name": "OperationalStatus",
            "modelAsString": true
          },
          "x-ms-mutability": [
            "read"
          ],
          "description": "Operational status of the Disk pool"
        },
        "tpgs": {
          "type": "array",
          "description": "list of iSCSI target portal groups",
          "items": {
            "$ref": "#/definitions/TargetPortalGroup"
          },
          "maxLength": 1,
          "minLength": 1
        },
        "targetIqn": {
          "type": "string",
          "description": "iSCSI target iqn."
        }
      }
    },
    "TargetPortalGroup": {
      "type": "object",
      "description": "iSCSI target portal group",
      "required": [
        "luns",
        "acls",
        "attributes"
      ],
      "properties": {
        "luns": {
          "type": "array",
          "description": "Lun list to be exposed through the iSCSI target. Required",
          "items": {
            "$ref": "#/definitions/IscsiLun"
          },
          "minLength": 1,
          "maxLength": 8
        },
        "acls": {
          "type": "array",
          "description": "Node acls for an iSCSI target",
          "items": {
            "$ref": "#/definitions/Acl"
          }
        },
        "attributes": {
          "$ref": "#/definitions/Attributes"
        },
        "endpoints": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-ms-mutability": [
            "read"
          ]
        },
        "tag": {
          "type": "integer",
          "description": "The tag associated with the iSCSI target portal group",
          "x-ms-mutability": [
            "read"
          ]
        },
        "port": {
          "type": "integer",
          "description": "The port at which the iSCSI target is available",
          "x-ms-mutability": [
            "read"
          ]
        }
      }
    },
    "Acl": {
      "type": "object",
      "description": "",
      "required": [
        "initiatorIqn",
        "mappedLuns"
      ],
      "properties": {
        "initiatorIqn": {
          "type": "string",
          "description": "iSCSI initiator IQN"
        },
        "mappedLuns": {
          "type": "array",
          "description": "Array of lun names mapped to the ACL",
          "items": {
            "type": "string"
          }
        },
        "credentials": {
          "description": "CHAP credentials for iSCSI target.",
          "x-ms-client-flatten": true,
          "x-ms-secret": true,
          "x-ms-mutability": [
            "create"
          ],
          "$ref": "#/definitions/IscsiTargetCredentials"
        }
      }
    },
    "Attributes": {
      "type": "object",
      "description": "Attributes of an iSCSI target",
      "additionalProperties": {
        "type": "boolean"
      },
      "x-ms-mutability": [
        "read",
        "create",
        "update"
      ]
    },
    "IscsiTargetCredentials": {
      "type": "object",
      "required": [
        "username",
        "password"
      ],
      "description": "CHAP credentials for an iSCSI target",
      "properties": {
        "username": {
          "type": "string",
          "description": "Username for CHAP authentication.",
          "minLength": 7,
          "maxLength": 128
        },
        "password": {
          "type": "string",
          "description": "Password for CHAP authentication",
          "minLength": 12,
          "maxLength": 512
        }
      }
    },
    "IscsiLun": {
      "type": "object",
      "required": [
        "name",
        "managedDiskAzureResourceId"
      ],
      "description": "Lun to expose the ManagedDisk.",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string",
          "minimum": 7,
          "maximum": 12,
          "description": "Lun name."
        },
        "managedDiskAzureResourceId": {
          "type": "string",
          "description": "Unique Azure resource id of the managed disk. Required."
        }
      }
    },
    "Error": {
      "type": "object",
      "description": "The resource management error response.",
      "properties": {
        "error": {
          "type": "object",
          "$ref": "./common-types.json#/definitions/ErrorResponse"
        }
      }
    }
  }
